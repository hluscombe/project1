<%= form_for @designer do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name, :autofocus => true, :required => true %>
  <%= f.label :dob, "Date of Birth" %>
  <%= f.text_field :dob %>
  <%= f.label :company, "Company" %>
  <%= f.text_field :company %>
  <%= f.label :image, "Photo" %>
  <%= f.url_field :image %>
  <%= f.submit %>
<% end %>
